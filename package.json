{
  "name": "g7",
  "version": "0.0.1",
  "description": "Easy to use, no additional configuration.",
  "main": "lib/index.ts",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "commit": "git-cz",
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "lint": "tslint --config tslint.json '{lib,gl-matrix}/**/*.ts(x)?'",
    "lint:fix": "tslint --config tslint.json '{lib,gl-matrix}/**/*.ts(x)?' --fix",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "BPMN",
    "Workflow",
    "Canvas",
    "TypeScript"
  ],
  "author": "Jiahao Li",
  "repository": {
    "type": "git",
    "url": "https://github.com/LiJiahaoCoder/g7.git"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitzen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/gl-matrix": "^3.2.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.12",
    "@types/prismjs": "^1.16.2",
    "@types/react-dom": "^16.9.9",
    "babel-loader": "^8.1.0",
    "babel-plugin-prismjs": "^2.0.1",
    "bootstrap": "^4.5.3",
    "chalk": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-shader-loader": "^1.0.6",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0"
  }
}
